<p id="notice"><%= notice %></p>

<p>
  <strong><%= t('task.title') %> :</strong>
  <%= @task.title %>
</p>

<p>
  <strong><%= t('task.content') %>:</strong>
  <%= @task.content %>
</p>

<p>
  <strong><%= t('task.status') %>:</strong>
  <%= @task.status %>
</p>

<p>
  <strong><%= t('task.priority') %>:</strong>
  <%= @task.priority %>
</p>

<p>
  <strong><%= t('task.expiration') %>:</strong>
  <%= @task.expiration %>
</p>

<p>
  <strong><%= "ラベル" %>:</strong>
  <% @task.task_labels.each do |task_label| %>
    <strong><%= task_label.label.title %><br></strong>
  <% end %>
</p>

<%= link_to t('task.Edit'), edit_task_path(@task) %> |
<%= link_to t('task.Back'), tasks_path %>